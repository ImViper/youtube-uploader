{
  "name": "youtube-videos-uploader",
  "version": "2.0.27",
  "description": "Uploads videos to youtube without any limits",
  "main": "dist/index.js",
  "types": "dist",
  "files": [
    "dist",
    "auth",
    "test"
  ],
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "@types/cors": "^2.8.19",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.1.0",
    "cors": "^2.8.5",
    "express": "5.1.0",
    "fs-extra": "^11.2.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "prettier": "^2.4.1",
    "puppeteer": "^14.4.1",
    "puppeteer-extra": "^3.3.0",
    "puppeteer-extra-plugin-stealth": "^2.10.1",
    "socket.io": "^4.8.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fawazahmed0/youtube-uploader.git"
  },
  "scripts": {
    "build": "tsc -p . && npm run copy:sql",
    "prepare": "npm run build",
    "format": "prettier --config .prettierrc \"src/**/*.ts\" --write",
    "start": "node dist/server.js",
    "start:simple": "node dist/server-simple.js",
    "dev": "npm run build && node dist/server.js",
    "dev:simple": "npm run build && node dist/server-simple.js",
    "server": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch",
    "init:browsers": "npm run build && node dist/scripts/init-browser-profiles.js",
    "check:browsers": "npm run build && node dist/scripts/check-browser-status.js",
    "db:migrate": "node dist/database/run-migrations.js",
    "build:all": "npm run build && npm run copy:sql",
    "copy:sql": "node scripts/copy-sql.js",
    "test:coverage": "jest --coverage",
    "test:task": "jest src/api/task/__tests__",
    "test:task:coverage": "jest src/api/task/__tests__ --coverage"
  },
  "keywords": [
    "youtube uploader",
    "No limits",
    "youtube upload",
    "videos",
    "youtube video upload",
    "youtube videos upload",
    "youtube",
    "video uploader",
    "video upload"
  ],
  "author": "Fawaz Ahmed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fawazahmed0/youtube-uploader/issues"
  },
  "homepage": "https://github.com/fawazahmed0/youtube-uploader#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.1",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.2",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3"
  }
}
